<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedAttribute">

    <!-- thin -->
    <font
        android:font="@font/noto_thin"
        android:fontStyle="normal"
        app:font="@font/noto_thin"
        app:fontStyle="normal"
        app:fontWeight="100" />

    <!-- light -->
    <font
        android:font="@font/noto_light"
        android:fontStyle="normal"
        app:font="@font/noto_light"
        app:fontStyle="normal"
        app:fontWeight="200" />

    <!-- demilight -->
    <font
        android:font="@font/noto_demilight"
        android:fontStyle="normal"
        app:font="@font/noto_demilight"
        app:fontStyle="normal"
        app:fontWeight="300" />

    <font
        android:font="@font/noto_regular"
        android:fontStyle="normal"
        app:font="@font/noto_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <font
        android:font="@font/noto_medium"
        android:fontStyle="normal"
        app:font="@font/noto_medium"
        app:fontStyle="normal"
        app:fontWeight="500" />

    <font
        android:font="@font/noto_bold"
        android:fontStyle="normal"
        app:font="@font/noto_bold"
        app:fontStyle="normal"
        app:fontWeight="600" />

    <font
        android:font="@font/noto_black"
        android:fontStyle="normal"
        app:font="@font/noto_black"
        app:fontStyle="normal"
        app:fontWeight="700" />
</font-family>